#: ../../Plugin.cs:69
msgid "->Mysql列出表格:SHOW TABLES;"
msgstr "->Mysql lists tables:SHOW TABLES;"

#: ../../Plugin.cs:68
msgid "->Sqlite列出表格:SELECT name FROM sqlite_master WHERE type='table';"
msgstr "->Sqlite lists tables: SELECT name FROM sqlite_master WHERE type='table';"

#: ../../Plugin.cs:75
msgid "->修改数据表指定内容:UPDATE <表格名字> SET <更新列名> = '更新值' WHERE <条件>"
msgstr "->Modify the specified content of the data table: UPDATE <table name> SET <update column name> = 'update value' WHERE <condition>"

#: ../../Plugin.cs:71
msgid "->删除表格:DELETE FROM <表格名字>;"
msgstr "->Delete table:DELETE FROM <table name>;"

#: ../../Plugin.cs:72
msgid "->删除记录:DELETE FROM <表格名字> WHERE <条件>;"
msgstr "->Delete records: DELETE FROM <table name> WHERE <condition>;"

#: ../../Plugin.cs:73
msgid "->查询表格内容:SELECT * FROM <表格名字>;"
msgstr "->Query table content: SELECT * FROM <table name>;"

#: ../../Plugin.cs:74
msgid "->查询表格内容扩展:SELECT * FROM <表格名字> WHERE <条件 > LIMIT <返回行数>;"
msgstr "->Query table content expansion: SELECT * FROM <table name> WHERE <condition> LIMIT <number of rows returned>;"

#: ../../Plugin.cs:70
msgid "->清空表格:DROP TABLE <表格名字>;"
msgstr "->Clear the table: DROP TABLE <table name>;"

