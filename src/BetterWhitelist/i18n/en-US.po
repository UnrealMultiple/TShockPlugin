msgid "-------[BetterWhitelist]-------"
msgstr ""

#: ../../Main.cs:52
msgid "/bwl add {name}, 添加玩家名到白名单中\n"
msgstr "/bwl add {name}, Add player name to the whitelist\n"

#: ../../Main.cs:53
msgid "/bwl del {name}, 将玩家移出白名单\n"
msgstr "/bwl del {name}, Remove player from the whitelist\n"

#: ../../Main.cs:56
msgid "/bwl false, 关闭插件\n"
msgstr "/bwl false, Disable the plugin\n"

#: ../../Main.cs:51
msgid "/bwl help, 显示帮助信息\n"
msgstr "/bwl help, Show help information\n"

#: ../../Main.cs:54
msgid "/bwl list, 显示白名单上的全部玩家\n"
msgstr "/bwl list, Show all players on the whitelist\n"

#: ../../Main.cs:57
msgid "/bwl reload, 重载插件"
msgstr "/bwl reload, Reload the plugin"

#: ../../Main.cs:55
msgid "/bwl true, 启用插件\n"
msgstr "/bwl true, Enable the plugin\n"

#: ../../BConfig.cs:19
msgid "你不在服务器白名单中！"
msgstr "You are not on the server whitelist!"

#: ../../Main.cs:147
msgid "删除成功!"
msgstr "Deleted successfully!"

